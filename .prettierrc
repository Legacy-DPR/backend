{
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "trailingComma": "all",
    "printWidth": 80,
    "tabWidth": 4,
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/(.*)$",
        "",
        "^[./]",
        "",
        "<TYPES>"
    ],
    "importOrderTypeScriptVersion": "5.0.0",
    "importOrderParserPlugins": ["typescript", "decorators-legacy"],
    "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
